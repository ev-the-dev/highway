{
  "name": "@ev-the-dev/highway",
  "version": "0.0.5",
  "description": "ESLint Configuration",
  "files": [
    "./",
    "rules/**",
    "configs/**"
  ],
  "main": "index.js",
  "scripts": {
    "compile": "npx swc ./rules -d dist/rules/ && npx swc ./configs -d dist/configs/ && npx swc ./index.ts -o dist/index.js && cp -r package.json package-lock.json node_modules dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ev-the-dev/highway.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "eslint"
  ],
  "author": "ev-the-dev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ev-the-dev/highway/issues"
  },
  "homepage": "https://github.com/ev-the-dev/highway#readme",
  "dependencies": {
    "@eslint/js": "^8.54.0",
    "@stylistic/eslint-plugin": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.12.0"
  },
  "bundleDependencies": [
    "@eslint/js",
    "@stylistic/eslint-plugin",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser"
  ],
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.96",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.9.1",
    "typescript": "^5.2.2"
  }
}
